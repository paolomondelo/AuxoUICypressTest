{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["cypress", "node"],
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.tsx", "cypress.config.ts"],
  "exclude": ["node_modules", "cypress/videos", "cypress/screenshots", "cypress/downloads"]
}
